{
  "name": "ymir-web",
  "version": "2.1.0.0114",
  "scripts": {
    "local": "APP_ROOT=../pages/demo1 umi dev",
    "analyze": "cross-env UMI_ENV=dev ANALYZE=1 umi dev",
    "start": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:local": "cross-env MOCK=none UMI_ENV=local umi dev",
    "start:mock": "cross-env UMI_ENV=local umi dev",
    "build": "cross-env UMI_ENV=dev umi build",
    "build:dev": "cross-env UMI_ENV=dev umi build",
    "build:prod": "cross-env UMI_ENV=prod umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "lint-staged": "lint-staged",
    "eslint": "eslint src/**/*",
    "prepare": "cd ../.. && npx husky install && npx husky add .husky/pre-commit \"cd ymir/web && npx lint-staged\""
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**/*.{js,ts,tsx}",
      "!./src/assets/**",
      "!./src/components/**",
      "!./src/pages/**",
      "!./src/layouts/**",
      "!./src/locales/**",
      "!./src/interface/**",
      "!**/**/__test__/**",
      "!**/__mock__/**",
      "!**/node_modules/**",
      "!**/.umi/**",
      "!**/.umi-production/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --tab-width 2 --write"
    ],
    "*.ts?(x)": [
      "prettier --tab-width 2  --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.32.6",
    "@antv/g6": "^4.5.3",
    "@antv/graphin": "^2.4.13",
    "@antv/graphin-components": "^2.4.0",
    "@antv/graphin-icons": "^1.0.0",
    "@umijs/preset-react": "1.x",
    "ahooks": "^3.7.2",
    "antd": "^4.20.0",
    "antd-img-crop": "^4.1.0",
    "axios": "^1.1.3",
    "color": "^4.2.0",
    "crypto-js": "^4.1.1",
    "echarts": "^5.2.2",
    "human-number": "^2.0.1",
    "react": "17.x",
    "react-dom": "17.x",
    "react-json-view": "^1.21.3",
    "react-xml-viewer": "^1.3.0",
    "socket.io-client": "^4.4.1",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.2",
    "@types/color": "^3.0.3",
    "@types/human-number": "^1.0.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.2",
    "redux-saga": "^1.1.3",
    "typescript": "^4.8.4",
    "yorkie": "^2.0.0"
  }
}
